IMAGE_VERSION=1.2.4-cluster

declare -A MYSQL_CLUSTER_VERSIONS
MYSQL_CLUSTER_VERSIONS["7.5"]=7.5.23
MYSQL_CLUSTER_VERSIONS["7.6"]=7.6.19
